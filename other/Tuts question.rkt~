#reader(lib "htdp-beginner-reader.ss" "lang")((modname |Tuts question|) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #t)))
(define (strings-equal? los)
  (cond [(empty? los) true]
        [(empty? (rest los)) true] 
        [else (and(string=? (first los) (first (rest los)))
                  (strings-equal (rest-los)))]))
                                                     